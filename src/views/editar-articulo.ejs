<%- include ('partials/_header')  %>

    <div class="container mt-5">
        <div class="row">
            <div class="col-md-5">
                <div class="card-body">
                    <form action="/update/<%= data.id %>" method="POST">

                        <div class="form-group">
                            <input type="text" name="nombre" value="<%= data.nombre %>" class="form-control">
                        </div>
                        <div class="form-group">
                            <input type="text" name="precio" value="<%= data.precio %>" class="form-control">
                        </div>
                        <div class="form-group">
                            <input type="text" name="topeMinimo" value="<%= data.topeMinimo %>" class="form-control">
                        </div>
                        <div class="form-group">
                            <input type="text" name="topeMaximo" value="<%= data.topeMaximo %>" class="form-control">
                        </div>

                        <button class="btn badge-primary" type="submit">Editar Producto</button>
                    </form>
                </div>
            </div>
        </div>
    </div>

    <%- include ('partials/_footer')  %>